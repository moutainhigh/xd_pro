			type="com.xiaodou.resources.model.forum.ForumPraiseModel" />
		<typeAlias alias="ForumRelateInfoModel"
			type="com.xiaodou.resources.model.forum.ForumRelateInfoModel" />
		<typeAlias alias="ForumCommentModel"
			type="com.xiaodou.resources.model.forum.ForumCommentModel" />
		<typeAlias alias="PushTaskModel"
			type="com.xiaodou.resources.model.push.PushTaskModel" />
		<typeAlias alias="CategoryRelationModel"
