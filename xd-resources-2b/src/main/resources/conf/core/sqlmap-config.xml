		<mapper resource="conf/mybatis/forum/forumPraiseModel.xml" />
		<mapper resource="conf/mybatis/forum/forumRelateInfoModel.xml" />
		<mapper resource="conf/mybatis/forum/forumCommentModel.xml" />
