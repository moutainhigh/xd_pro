		<typeAlias type="com.xiaodou.resources.model.quesbk.RaiseWrongQues"
			alias="RaiseWrongQues" />
		<!-- 挑战记录 -->
		<typeAlias type="com.xiaodou.resources.model.quesbk.UserChapterRecord"
			alias="UserChapterRecord" />
		<typeAlias
			type="com.xiaodou.resources.model.quesbk.UserCourseStatisticDayModel"
			alias="UserCourseStatisticDayModel" />
