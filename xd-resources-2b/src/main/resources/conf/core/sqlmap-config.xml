			type="com.xiaodou.resources.model.reward.RewardRecord" />


		<!-- 题库 -->
		<typeAlias type="com.xiaodou.resources.model.quesbk.QuesbkBase"
			alias="QuesbkBase" />
		<!-- 题库练习试卷 -->
		<typeAlias type="com.xiaodou.resources.model.quesbk.QuesbkExamPaper"
			alias="QuesbkExamPaper" />
