		<mapper resource="conf/mybatis/quesbk/QuesbkQuesTypeMapper.xml" />
		<!-- 题库用户个人练习记录 -->
		<mapper resource="conf/mybatis/quesbk/UserExamRecordMapper.xml" />
		<!-- 题库用户个人练习情况统计 -->
		<mapper resource="conf/mybatis/quesbk/UserExamTotalMapper.xml" />
