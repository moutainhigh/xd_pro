          <div class="form-group">
            <label for="is_physical" class="col-sm-4 control-label">是否为实体商品</label>
            <div class="col-sm-6">
              <input type="radio" <%=obj.is_physical==true||obj.is_physical==undefined?'checked':''%> value="1" name="is_physical">是
              <input type="radio" <%=obj.is_physical==false?'checked':''%> value="0" name="is_physical">否
            </div>
          </div>
        </div>
