<div class="modal-body no-p">
  <div class="row">
    <div class="col-xs-12">
      <section class="panel panel-primary no-b">
        <header class="panel-heading clearfix brtl brtr">
          <div class="overflow-hidden">
            <a href="javascript:;" class="h4 show no-m pt5"><%=obj.name||''%></a>
          </div>
        </header>
        <div class="list-group">
          <a href="javascript:;" class="list-group-item" title="权限标识">
            <i class="ti-key mr10 text-muted"></i><%=obj.permission||''%>
          </a>
          <a href="javascript:;" class="list-group-item" title="URL">
            <i class="ti-world mr10 text-muted"></i><%=obj.url||''%>
          </a>
          <a href="javascript:;" class="list-group-item" title="启用状态">
            <i class="ti-unlock mr10 text-muted"></i>
            <% if(!obj.disabled){ %>
            <i class="fa fa-circle text-success ml5"></i>
            <% }else{ %>
            <i class="fa fa-circle text-danger ml5"></i>
            <% } %>

          </a>
          <a href="javascript:;" class="list-group-item" title="录入时间">
            <i class="ti-time mr10 text-muted"></i><%=obj.createdAt?moment(obj.createdAt*1000).format("YYYY-MM-DD HH:mm:ss"):''%>
          </a>
        </div>
      </section>
    </div>
  </div>
</div>
<div class="modal-footer">
  <button type="button" class="btn btn-primary btn-block" data-dismiss="modal">知道了</button>
</div>
