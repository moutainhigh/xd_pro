<div class="container-fluid">
  <!-- Page Heading -->
  <div class="row">
    <div class="col-lg-12">
      <h1 class="page-header">
        调度策略 <small>调度策略列表</small>
      </h1>
      <ol class="breadcrumb">
        <li class="active">
          <i class="fa fa-desktop"></i> 调度策略
        </li>
        <a href="#addScModal" role="button" style="float: right"  class="btn btn-primary" data-toggle="modal">添加策略</a>
      </ol>
    </div>
  </div>
  <!-- /.row -->

  <div class="row">
    <div class="col-lg-12">
      <div style="height: 10px"></div>
      <div class="panel panel-default">
        <div class="panel-body">
          <div class="table-responsive">
            <table id="taskTable" class="table table-bordered table-hover table-striped">
              <thead>
              <tr>
                <th class="col-lg-2">日志ID</th>
                <th class="col-lg-4">日志时间</th>
                <th class="col-lg-6">日志报告</th>
              </tr>
              </thead>
              <tbody>
              <% if (data) { %>
              <% data.forEach(function(record,index){ %>
              <tr>
                <td ><%-record.log_id%></td>
                <td><%-record.createdAt%></td>
                <td><a target=_blank href="/LogSchedule/log?log_id=<%-record.log_id%>">查看报告</a></td>
                <td><%-record.log_id%></td>
              </tr>
              <% }) %>
              <% } %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- /.row -->

</div>

<!-- /.container-fluid -->
<script src="/xd-autotest-framework/js/index/frame-right.js"></script>
<script src="/xd-autotest-framework/js/schedule/log.js"></script>
